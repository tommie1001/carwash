{
    "name": "swisnl/carwash",
    "description": "Data scrubber for Laravel applications",
    "type": "library",
    "require": {
        "laravel/framework": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "phpunit/phpunit" : "^8.5",
        "orchestra/testbench" : "^4.0|^5.0|^6.0",
        "orchestra/database": "^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4" : {
            "Carwash\\" : "src"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Carwash\\" : "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Soppelsa",
            "email": "dsoppelsa@gmail.com"
        }
    ],
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Carwash\\CarwashProvider"
            ]
        }
    }
}
